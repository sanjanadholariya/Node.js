                                      <th scope="col">City</th>
                                      <th scope="col">Action</th>
                                    </tr>
                                  </thead>
                                  <tbody>
                                    <%data.map((v,i)=>{%>
                                        <tr>
                                            <td>
                                                <img src="../uploads/<%=v.profile%>" width="35px" height="35px" style="border-radius: 50%;" alt="">
